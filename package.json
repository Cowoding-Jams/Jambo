{
	"name": "jambo",
	"version": "1.0.0",
	"description": "A bot to manage a whole game jam server from a fully functional poll-system to managing color roles.",
	"main": "dist/start.js",
	"private": "true",
	"type": "module",
	"scripts": {
		"lint": "eslint ./src/",
		"format": "npx prettier -w .",
		"tidy": "npm run lint && npm run format",
		"build": "tsc -p tsconfig.json",
		"run": "node --experimental-specifier-resolution=node .",
		"start": "npm run build && npm run run",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Cowoding-Jams/Jambo.git"
	},
