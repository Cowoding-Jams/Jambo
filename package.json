		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"husky": "^8.0.3",
		"prettier": "^2.8.3",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.34",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"enmap": "github:Cowoding-Jams/enmap",
		"luxon": "^3.2.1",
		"node-cron": "^3.0.2",
		"node-fetch": "^3.3.0",
		"winston": "^3.8.2"
	}
